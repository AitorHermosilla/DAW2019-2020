Algoritmo Notas
	
	Escribir "Introduce la nota"
	leer n
	
	si n >= 0 y n<4
		Escribir "Muy Insuficiente"
		
	SiNo
		si n<5
			Escribir "Insuficiente"
			
		SiNo
			si n = 5
				Escribir "Suficiente"
				
			SiNo
				si n < 7
					Escribir "Bien"
				SiNo
					si n=10
						Escribir "Sobresaliente"
					SiNo
						Escribir "Notable"
					FinSi
				FinSi
			FinSi
			
		FinSi
		
	FinSi
	
	
FinAlgoritmo
